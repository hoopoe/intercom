                <button type="button" class="btn cv"/>
              </td>
            </tr>
            <tr>
              <td>
                <b><%=l(:project_title)%>:</b>
              
                <div class="editable profile-data" data-button-class="default" data-prop="project">
                  <%% if (($.parseJSON(user.data) !== null) && ($.parseJSON(user.data).project)) { %>
                    <%%- $.parseJSON(user.data).project %>
                  <%% } else {%>
                    Put project here
                  <%% } %>
                </div>
              </td>
