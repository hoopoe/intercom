<% content_for :header_tags do %>
  <%= javascript_include_tag 'jquery-1.11.0.min.js', 'underscore-min.js', 'backbone-min.js', 'bootstrap.min.js', 'infiniScroll.js', 'etch.js', 'utils.js', 'topView.js', 'settingsView.js', 'eventsView.js', 'profileView.js', 'employersView.js', 'tercomin.js', :plugin => 'tercomin' %>
  <% if @themeName == 'bootswatch' %>
    <%= stylesheet_link_tag 'bootstrap.min.css', 'bootstrap.css.map', 'etch.css', 'tercomin.css', :plugin => 'tercomin' %>
  <% else %>
    <%= stylesheet_link_tag 'etch.css', 'tercomin-classic.css', :plugin => 'tercomin' %>
  <% end %>
<% end %>
<div class="navbar navbar-default">
  <div class="navbar-header">
    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-responsive-collapse">
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
    <a class="navbar-brand" href="#">
      <%= image_tag 'logo.png', :plugin => 'tercomin' %>
    </a>
  </div>
  <div class="collapse navbar-collapse top-menu">
    <div class="navbar-form navbar-right">
      <input type="text" class="form-control col-lg-8 search-control" placeholder="поиск">
    </div>
    <div class="demo_lbl">
      <span>Demo. All changes will be reverted.</span>
    </div>
    <ul class="nav navbar-nav">
      <li class="menu-item" value="mypage"><a href="#mypage"><%=l(:my_page)%></a></li>
      <li class="menu-item" value="employers"><a href="#employers"><%=l(:employers)%></a></li>
      <li class="menu-item" value="events"><a href="#events"><%=l(:events)%></a></li>
      <li class="menu-item" value="settings"><a href="#settings"><%=l(:settings)%></a></li>
    </ul>
  </div><!--/.nav-collapse -->
</div>
<!--%= render :partial => 'classic' %-->
<div id="container-fluid" >
  <div class="col-md-1">
  </div>
  <div class="col-md-10 content">
  </div>
  <div class="col-md-1">
  </div>
</div>
<script type="html/template" id="settings-template">
  <div class="theme-picker">
    <select class="form-control theme-select">
      
       <%% if ((user.settings) && ($.parseJSON(user.settings) !== null) 
        && ($.parseJSON(user.settings).theme !== null) && ($.parseJSON(user.settings).theme == "bootswatch")) { %>
              <option value="default" >default</option>
              <option value="bootswatch" selected="selected"> bootswatch</option>
        <%% } else {%>
              <option value="default" selected="selected">default</option>
              <option value="bootswatch"> bootswatch</option>
        <%% } %>
          
    </select>    
  </div>
</script>
<script type="html/template" id="events-template">
  <div>
    Event item
  </div>
</script>
<script type="html/template" id="employer-template">
  <div class="row">
    <div class="col-md-12">
      <div class="row employer_row">
        <div class="col-md-2 employer_img">
           <a href='#mypage/<%%- user.id %>'>
              <%% if (user.avatar_url == "noavatar") { %>
  
                <img class="no-profile-img"/>
              <%% } else {%>
                <img src=<%%- user.avatar_url %> alt="" class="employer-img" />
              <%% } %>
           </a>
        </div>
        <div class="col-md-10">
          <label><%%- user.lastname %></label>
          <label><%%- user.firstname %></label>
          <br>
          <label>e-mail:</label>
          <label><%%- user.mail %></label>
          <br>
          <label><%=l(:project_title)%>:</label>
          <%% if ($.parseJSON(user.data) !== null) { %>
             <label><%%= $.parseJSON(user.data).project%></label>
          <%% } %>
          <br>
          <label><%=l(:project_extra_title)%>:</label>
          <%% if ($.parseJSON(user.data) !== null) { %>
             <label><%%= $.parseJSON(user.data).project_extra%></label>
          <%% } %>
          <br>
          <label><%=l(:room_number)%>:</label>
          <%% if ($.parseJSON(user.data) !== null) { %>
             <label><%%= $.parseJSON(user.data).room_number%></label>
          <%% } %>
        </div>
      </div>
      <hr>
    </div>
  </div>
</script>
<script type="text/template" id="profile-template">
  <div class="row">
    <div class="col-md-12">
        <div class="col-md-4">
          <div class="profile-div">
            <%% if (user.avatar_url === "noavatar") { %>
              <img class="profile-img no-profile-img"/>
            <%% } else {%>
              <img src=<%%- user.avatar_url %> alt="" class="profile-img" />
            <%% } %>
            <div class="profile-img-save profile-img-save-hide">
              <button type="button" class="btn btn-primary">Save</button>
            </div>
          </div>
        </div>
        <div class="col-md-8">
          <table>
